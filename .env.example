# Copy this file to .env and fill in the values.
# Never commit .env to version control.

# URL of the backend API service, without a trailing slash.
# Used by nginx to proxy /api/* requests.
# When the backend runs in the same Docker Compose project, use
# the Compose service name, e.g.:
#   BACKEND_URL=http://backend:8000
# For an external backend, use the full URL, e.g.:
#   BACKEND_URL=https://api.example.com
BACKEND_URL=http://backend:8000

# Host port to expose the frontend on (default: 80).
# Change this to run the frontend on a different port, e.g.:
#   FRONTEND_PORT=5555
FRONTEND_PORT=80
